    if (response.data.operadoras && Array.isArray(response.data.operadoras)) {\n            this.operadoras = response.data.operadoras;\n          } else {\n            this.operadoras = [];\n          }\n        }\n      } catch (error) {\n        console.error(\"Erro ao buscar operadoras:\", error);\n        this.operadoras = [];  // Em caso de erro, limpar a lista de operadoras\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n#app {\n  font-family: Arial, sans-serif;\n  padding: 20px;\n}\n\ninput {\n  padding: 10px;\n  font-size: 16px;\n  margin-bottom: 20px;\n  width: 300px;\n}\n\nh2 {\n  margin-top: 20px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}